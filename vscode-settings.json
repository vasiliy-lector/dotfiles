{
  "module-templates.engine": "handlebars",
  "module-templates.templates": [
    {
      "displayName": "Module",
      "defaultPath": "src/modules",
      "folder": "{{pascal name}}",
      "questions": {
        "name": "Component name"
      },
      "files": [
        {
          "name": "{{pascal name}}.tsx",
          "content": [
            "// import styles from './{{pascal name}}.module.css';",
            "",
            "import React from 'react';",
            "",
            "type Props = {};",
            "",
            "const {{pascal name}}: React.FC<Props> = () => {",
            "    return null;",
            "};",
            "",
            "export default {{pascal name}};",
            "",
          ]
        },
        {
          "name": "{{pascal name}}.module.css",
          "content": [
            " "
          ]
        }
      ]
    },
    {
      "displayName": "Component",
      "defaultPath": "src/components",
      "questions": {
        "name": "Component name"
      },
      "files": [
        {
          "name": "{{pascal name}}.tsx",
          "content": [
            "import React from 'react';",
            "",
            "type Props = {};",
            "",
            "const {{pascal name}}: React.FC<Props> = () => {",
            "    return null;",
            "};",
            "",
            "export default {{pascal name}};",
            "",
          ]
        }
      ]
    }
  ],
  "css.lint.unknownAtRules": "ignore",
  "css.lint.validProperties": [
    "composes"
  ]
}